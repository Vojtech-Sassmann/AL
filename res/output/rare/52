def weeks(n):
    print("Pocet dnu: {}".format(n*7))
    print("Pocet hodin: {}".format(n*7*24))
    print("Pocet minut: {}".format(n*7*24*60))

184.0
def weeks(n):
    print("Pocet dnu: " + str(n*7))
    print("Pocet hodin: " + str(n*7*24))
    print("Pocet minut: " + str(n*7*24*60))

178.0
def weeks(n):
    days = n * 7
    hours = n * 7 * 24
    minutes = n * 7 * 24 * 60
    print("Pocet dnu:", days)
    print("Pocet hodin:", hours)
    print("Pocet minut:", minutes)

