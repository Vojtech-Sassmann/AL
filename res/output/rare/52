def weeks(n):
    dny = n*7
    print("Pocet dnu:", dny)
    hodin = n*7*24
    print("Pocet hodin:", hodin)
    minut = n*7*24*60
    print("Pocet minut:", minut)

157.0
def weeks(n):
    days = n * 7
    print("Pocet dnu:", days)
    hours = int(days) * 24
    print("Pocet hodin:", hours)
    minutes = int(hours) * 60
    print("Pocet minut:", minutes)

