def multiples(n):
    dvoj = n + n
    troj = n + n + n
    deseti = n + n + n + n + n + n + n + n + n + n
    print("Dvojnasobek", dvoj)
    print("Trojnasobek", troj)
    print("Desetinasobek", deseti)

