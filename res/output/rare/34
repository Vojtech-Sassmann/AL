def multiples(n):
    n+=n
    print("Dvojnasobek", n)
    n+=n//2
    print("Trojnasobek", n)
    n=(n//3)*10
    print("Desetinasobek", n)

247.0
def multiples(n):
    n = n * 2
    print("Dvojnasobek", int(n))
    n = (n * 3) / 2
    print("Trojnasobek", int(n))
    n = (n * 10) / 3
    print("Desetinasobek", int(n))
     

