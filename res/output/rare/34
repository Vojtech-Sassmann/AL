def multiples(n):
    print("Dvojnasobek", n+n)
    print("Trojnasobek", n+n+n)
    print("Desetinasobek", n+n+n+n+n+n+n+n+n+n)
321.0
def multiples(n):
    nas = [2,3,10]
    slov = ["Dvojnasobek", "Trojnasobek", "Desetinasobek"]
    j = n
    for i in range(3):
        n = j
        n *= nas[i]
        print(slov[i], n)

307.0
def multiples(n):
    words = ["Dvojnasobek", "Trojnasobek", "Desetinasobek",]
    numba = [2, 3, 10]
    for i in range(0,3):
        m=numba[i]*n
        print(words[i], m)
       

270.0
def multiples(n):
    for i in range(1, 11):
        if i == 2:
            print("Dvojnasobek", i*n)
        elif i == 3:
            print("Trojnasobek", i*n)
        elif i == 10:
            print("Desetinasobek", i*n)

