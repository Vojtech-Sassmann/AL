def make_fancy(text, n):
    i = 0    
    for letter in text:
    	print(" "*i, end = '')
    	i = i + 1
    	for word in range(0, n):
        	print(letter + " ", end = '')
    	print()
          

183.0
def make_fancy(text, n):
    p_radku = len(text)
    x = " "
    for i in range(p_radku):
        print(x + n*(text[i] + " "))
        x = x + " "

