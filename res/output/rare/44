def vypis_radek(pismenko, opakovani, mezer_na_zacatku):
    print(" " * mezer_na_zacatku, end="")
    print(" ".join(pismenko * opakovani))

def make_fancy(text, n):
    for i in range(len(text)):
        pismenko = text[i]
        opakovani = n
        mezer_na_zacatku = i
        vypis_radek(pismenko, opakovani, mezer_na_zacatku)

193.0
def make_fancy(text, n):
    counter=0
    for a in text:
        print(counter*" ",end="")
        for b in range (n):
            print(a,"",end='')
        print()    
        counter+=1


190.0
def make_fancy(text, n):
    i = 0
    for char in text:
        print(i*" ", end="")
        for _ in range(n):
            print(char+" ", end="")
        i += 1
        print()
        

