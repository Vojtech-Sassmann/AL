671.0
def digit_sum(n):
    k = len(str(n))
    sum = n % 10
    j=1
    for i in range(1,k):
        j=j*10
        sum = (n//j)%10 + sum
    return sum

658.0
def digit_sum(n):
    b = 0
    while n != 0:
        c = n%10
        b += c
        n -= c
        n = int(n) / 10
    return int(b)
        
        

639.0
def digit_sum(n):
    return (sum(int(digit) for digit in str(n)))

601.0
def digit_sum(n):
    l = []
    k = 0
    for i in str(n):
        l.append(i)
    for i in l:
        k += int(i)
    return k
598.0
def digit_sum(n):
    sucet=0
    cifra=0
    while n >0:
        cifra=n%10
        sucet=sucet+cifra
        n=(n-cifra)/10
        
    return int(sucet)

594.0
def digit_sum(n):
    string = str(n)
    arr = list(string)
    sum = 0
    for i in range(len(arr)):
        sum+=int(arr[int(i)])
    return sum    

