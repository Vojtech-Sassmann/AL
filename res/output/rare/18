558.0
def digit_sum(n):
    counter=0
    string=str(n)
    length=len(string)
    for i in range(length):
        number=string[i]
        real_number=int(number)
        counter=counter+real_number
        
    return counter
	
496.0
def digit_sum(n):
    if n == 0:
        return 0
    count = 0
    num = n
    for no in range(len(str(n))):
        count += num % 10
        num = num // 10
    return count
486.0

def digit_sum(n):
    x = 0
    count = list(map(int, str(n)))
    for i in range(len(count)):
        x += count[i]
    return x

