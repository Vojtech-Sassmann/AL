def dragon_test(heads, pigs):
    if heads>pigs:
        if heads%pigs==0:
            print("Nebude bitka.")
        else:
            print("Bude bitka.")
    elif pigs%heads==0:
        print("Nebude bitka.")
    else:
        print("Bude bitka.")

