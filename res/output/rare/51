def dragon_test(heads, pigs):
    if pigs > heads:
        if (pigs % heads) == 0:
        	print("Nebude bitka.")
        else:
        	print("Bude bitka.")           
    elif pigs == heads:
        print("Nebude bitka.")
    else:
        print("Bude bitka.")

