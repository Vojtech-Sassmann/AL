0.9427588364091336
def cross(n):    
    for i in range(1,3*n+1):
        for j in range(1,3*n+1):
            if n+1 <= i <= 2*n:
                print("#", end="")
            else: 
                if n+1 <= j <= 2*n :
                	print("#", end="")
                else : print(".", end="")
        print()

0.8601814200813263
def cross(n):    
    for i in range(1,(n*3)+1):
        for j in range(1,(3*n)+1):
            if ((i >= n+1) and (i<=2*n)) or ((j >= n+1) and (j<=2*n)):
                print("#", end="")
            else:
                print(".", end="")
        print()

