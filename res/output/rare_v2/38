def cross(n):    
    for i in range(3*n):
        for j in range(3*n):
            if (i < n or i >= 2*n) and (j < n or j >= 2*n):
                print(".", end="")
            else:
                print("#", end="")
        print()

0.8085705348764467
def cross(n):
    r = range(n, n+n)
    for i in range(3*n):
        for j in range(3*n):
            if i in r or j in r:
                print("#", end="")
            else:
                print(".", end="")
        print()

