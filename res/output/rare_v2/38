1.8751954957772914
def cross(n):
    for i in range(n):
        print(n*".", end = "")
        print(n*"#", end = "")
        print(n*".", end = "")
        print()
    for i in range(n):
        print(n*"#", end = "")
        print(n*"#", end = "")
        print(n*"#", end = "")
        print()
    for i in range(n):
        print(n*".", end = "")
        print(n*"#", end = "")
        print(n*".", end = "")
        print()
1.3883328120112606
def cross(n):    
    for i in range(n):
        print("."*n, end="")
        print("#"*n,end="")
        print("."*n,end="")
        print()
    for i in range(n):
        print("#"*n*3)
    for i in range(n):
        print("."*n, end="")
        print("#"*n,end="")
        print("."*n,end="")
        print()

1.3126368470441039
dot = "."
hs = "#"

def cross(n):    
	for i in range(n):
		print(n*dot+n*hs+n*dot, end="")
		print()
	for i in range(n):
		print(n*3*hs, end="")
		print()
	for i in range(n):
		print(n*dot+n*hs+n*dot, end="")
		print()
