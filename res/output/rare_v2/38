def cross(n):    
        for i in range(1,n+1):
            print(n*"."+n*"#"+n*".")
        for k in range(n,2*n):
            print((3*n)*"#")
        for j in range(2*n,(3*n)):
            print(n*"."+n*"#"+n*".")
        
        
			 

          
1.2541445104785738
def cross(n):    

    for row in range(1,3*n+1):
        if row<n+1 or row>2*n:
            for col in range (1,3*n+1):
                if col<n+1 or col>2*n:
                    print (".", end="")
                else:	print ("#", end="")
            print ()
        else:
            for i in range(1,3*n+1):
                print("#",end="")
            print()
        
    	
			
    
1.2369408820769472
def cross(n):    
    for i in range(n):
        print(("." * n) + ("#" * n) + ("." * n))
    for i in range(n):
        print("#" * (3 * n))
    for i in range(n):
        print(("." * n) + ("#" * n) + ("." * n))
1.228339067876134
def leg(n):
    for l in range(n):
        print("." * n + "#" * n + "." * n)
        
def cross(n):
    leg(n)
    for c in range(n):
        for s in range(3):
        	print("#"*n, end="")
        print()
    leg(n)
        
