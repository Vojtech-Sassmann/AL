def check(p, k):
    if p % (k*4) == 0:
        print("OK")
    else:
        if (p - (k*4)) > 0:
            print("Prebyva:",p - (k*4))
        if (p - (k*4)) < 0:
            print("Chybi:",-1 * (p - (k*4)))
