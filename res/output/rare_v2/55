def check(p, k):
    if k*4 == p:
    	print("OK")
    elif k*4 > p:
        chybi = k*4 - p
        print("Chybi: " + str(chybi))
    else:
        prebyva = p - k*4
        print("Prebyva: " + str(prebyva))

