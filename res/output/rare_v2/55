1.0947368421052632
def check(p, k):
    surplus = (p % 4)
    shoes = (k * 4)
    missing = (shoes - p)
    
    if (p > shoes):
    	print('Prebyva:', surplus)
    elif (p < shoes):
        print('Chybi:', missing)
    else:
        print('OK')

