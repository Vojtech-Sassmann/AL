1.6792906178489704
def check(p, k):
    if(p%k!=0):
        if(k*4<p):
            print("Prebyva:",p-(k*4))
        else:
            print("Chybi:",(k*4)-p)
    elif(p%k==0 and k*4!=p):
        print("Prebyva:",p-(k*4))
    else:
        print("OK")
  

1.5216247139588102
def check(p, k):
    if k%(p/4)==0:
        print("OK")
    else:
        if (4*k)-p<0:
            print("Prebyva: "+str(((4*k)-p)*-1))
        elif (4*k)-p>0:
            print("Chybi: "+str(((4*k)-p)))

