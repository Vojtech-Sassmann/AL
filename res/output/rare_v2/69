def factorize(n):
    print(n, '=',end=(' '))
    for i in range(n):   	
        while n%(i+1)==0 and (i+1)!=1 and n//(i+1)!=1:
            n=n//(i+1)
            print(i+1,'*',end=(' '))
    print(n,end=' ')
    print()
        	
          
            


0.9036591833127099
def factorize(n):
    print(n, "=", end=" ")
    while n!=1:
        x=1
        while n%x!=0 or x==1:
            x+=1
            if n%x==0:
                print(x, end=" ") 
        n=n//x
        if n!=1:
            print("*", end=" ")
    print()
        

