def factorize(n):
    p=2
    print(n,"=", end=" ")
    x=n
    while IsPrime(n)!=True:
        if n%p==0 :
            n=n//p
            print (p,end=" * ")
            
        else: p+=1
                
    print (n)            
    

def IsPrime(p):
    c=0
    for i in range (1,p+1):
        if p%i==0:
            c+=1
    if c==2:
        return True
0.9816156973660951
def factorize(n):
    a = n
    statement = ''
    while a != 1:
        for i in range(2, a+1):
            while a % i == 0 and i != a:
                statement += str(i) + ' * '
                a //= i
            if i == a:
                statement += str(i)
                a //= i
    print(n, "=", statement)

