def factorize(n):
    oldn = n
    moj = []
    x = 2
    while(x < 1000):
        if n % x == 0:
            moj.append(x)
            n /= x
            x = 2
        else:
            x += 1
    print(oldn, "= ", end="")        
    for i in range(len(moj)):
        if i == 0:
            print(moj[i], end=" ")
        else:
            print("*",moj[i], end = " ")
    print()
