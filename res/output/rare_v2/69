import math

def is_prime(n):
    if n < 2:
        return False
    for k in range(2, int(math.sqrt(n)) + 1):
        if n % k == 0:
            return False
    return True

def factorize(n):
    curr_num = n
    divs = []
    max_prime_div = 1
    while curr_num != 1:
        for i in range(1, curr_num+1):
            if is_prime(i) and curr_num % i == 0:
                max_prime_div = i
        divs.append(max_prime_div)
        curr_num //= max_prime_div
    print(n, "=", divs[-1], end=" ")
    for i in range(len(divs)-1):
    	print("*", divs[-i-2], end=" ")

