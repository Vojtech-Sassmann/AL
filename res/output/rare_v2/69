def factorize(n):
    print(n, "= ", end="")
    alist = []
    i = 1
    while n > 1:
        if isprimetest(i) is True:
            if n % i == 0:
                alist.append(i)
                n = n // i
            else:
                i = i + 1
        else:
                i = i +1
    print(*alist, end='', sep=' * ')
    print()


def isprimetest(x):
    count = 0
    for i in range(1, x):
        if x % i == 0:
            count = count + 1
    if count == 1:
        return True
    else:
        return False
