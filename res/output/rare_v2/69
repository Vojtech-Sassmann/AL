0.7779741912674563
def factorize(n):
    x = int(n)
    c = []
    for i in range(2, n+1):
        while x % i == 0:
            c.append(i)
            x = x // i
    print(n, "=", c[0],end= "")
    for i in range (1,len(c)):    
        print(" *", c[i], end="")

