def factorize(n):
    on=n
    s=""
    a=2
    while n>1:
        if n%a==0:
            n=n//a
            s+=str(a)+" * "
        else:
            a+=1
           
        
    print(on, "=", s[0:len(s)-3])

0.8813858935831713
def factorize(n):
    i = 2
    print(str(n) + " = ", end="")
    while i * i <= n:
        if n % i:
            i += 1
        else:
            n //= i
            print(i, end=" * ")
    if n > 1:
        print(n)

