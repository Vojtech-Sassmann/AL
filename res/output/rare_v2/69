def factorize(n):
    retez=str(n)+' = '
    k=n
    j=1
    for i in range(1,k+1):
            if ((k%i)==0) & (i>1):
                while (k%i==0) & (n>=i*j):
                    k = int(k//i)
                    if j*i==n:
                        retez = retez + str(i)
                    else:
                        retez = retez + str(i) + " * "
                    j= j * i
    if n == k:
          print(n, "=")
    else:
        print(retez)
            
1.1470744210712391
def factorize(n):
    primDiv = []
    num = n
    while num % 2 == 0:
        num = num//2
        primDiv.append(2)
#        print(num)
#    print(primDiv)
    
    for i in range(3, num+1, 2):
        while num % i == 0:
            num = num//i
            primDiv.append(i)
#            print(num)
#    print(primDiv)
    print('{} = '.format(n), end='')
    for item in primDiv[:-1]:
        print('{} * '.format(item), end='')
    print(primDiv[-1])
1.1423015732720523
def factorize(n):
    new  =[]
    m = n
    t = True
    while t:
        d = 2
        p = True
        while p:
            if m%d == 0:
                new.append(d)
                m = int(m/d)
                p = False
            else:
                d += 1
                
        if m == 1:
            t = False
            
    print(n, "= ", end="")
    for i in range(len(new)):
        print(new[i], end="")
        if i != len(new) - 1:
            print(" * ", end="")
    print()
