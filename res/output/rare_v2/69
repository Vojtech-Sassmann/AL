def factorize(n):
    x = list()
    y = list()
    for i in range(2,n+1):
        for j in range(2,i):
            if i %j == 0:
                break
        else:
            x.append(i)
#    print(x)
    index = 0
    n_orig = n
    while index < (len(x)):
        if n % x[index] == 0:
            n = n // x[index]
            y.append(x[index])
        else:
            index = index+1
    print(n_orig, "=", end=' ')
    for i in range(0, len(y)):
        if i < (len(y)-1):
            print(y[i],"*",end=' ')
        else:
            print(y[i], end=' ')

