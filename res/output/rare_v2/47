def sum_dif(a, b):
    print(str(a) + " + " + str(b) + " = " + str(a+b))
    print(str(a) + " - " + str(b) + " = " + str(a-b))

1.2013651877133107
def sum_dif(a, b):
    print("{a} + {b} = {res}".format(a=a, b=b, res=a+b))
    print("{a} - {b} = {res}".format(a=a, b=b, res=a-b))

0.9556313993174061
def sum_dif(a, b):
    print(a, "+", b, "=", sum((int(a),int(b))))
    print(a, "-", b, "=", int(a)-int(b))
    

0.9392491467576792
def sum_dif(a, b):
    plus = a + b
    minus = a - b
    print("%d + %d = %d" % (a, b, plus))
    print("%d - %d = %d" % (a, b, minus))
0.9392491467576792
def sum_dif(a, b):
    print("{} + {} = {}".format(a,b,(a+b)))
    print("{} - {} = {}".format(a,b,(a-b)))     

