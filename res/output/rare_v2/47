1.769283276450512
def sum_dif(a, b):
    print(a,end=" + ")
    print(b,end=" = ")
    print(a+b)
    print(a,end=" - ")
    print(b,end=" = ")
    print(a-b)

1.5017064846416381
def sum_dif(a, b):
    soucet = a + b
    rozdil = a - b
    print(str(a) + " + " + str(b) + " = " + str(soucet))
    print(str(a) + " - " + str(b) + " = " + str(rozdil))

