def queen(x, y):
    for j in range(1,9):
      for i in range(1,9):
        dx=i-x
        dy=j-y
        if dx==0 and dy==0:
            z="D"
        elif dx*dy==0 or abs(dx)==abs(dy):
            z="*"
        else:
            z="."
        print(z,end="")
      print()  

1.2305034550839091
def queen(x,y):
    n = y
    for j in range(1,9):
        n = n - 1
        for i in range(1,9):
## dama
            if i == x and j == y:
                print("D", end ="")
## okoli +1 pole
            elif i == x or j == y or ((i == x - 1 or i == x + 1) and (j == y - 1 or j == y + 1)):
                print("*", end ="")
## diagonaly
            elif j == y - n and i == x - n or j == y - n and i == x + n:
                print("*", end ="")
##            elif j == y - n and i == x + n:
##                print("o", end =" ")
            else:
                print(".", end ="")
        print()
1.1994076999012833
def queen(x, y):
    zmen=x
    x=y
    y=zmen
    pom=y-x
    pom2=y+x-2
    for i in range(8):
        for j in range(8):
            if i+1==x and j+1==y:
                print("D", end='')
            elif i+1==x or j+1==y or pom==j or pom2==j:
                print("*", end='')
            else:
                print(".", end='')
        print()
        pom=pom+1
        pom2=pom2-1

