def queen(x, y):

    for i in range(1,9):
        for j in range (1,9):
            if i==y and j==x:
                print("D",end="")
            elif i==y and j!=x:
                print("*",end="")
            elif j==x and i!=y:
                print ("*",end="")
            elif i-y==j-x:
                print ("*",end="")
            elif i-y==x-j:
                print("*",end="")
                
            else:print (".",end="")
        print()

0.9595261599210266
def queen(x, y):
    size = 8
    for i in range(0, size):
        for j in range(0, size):
            if i == y-1 and j == x-1:
                print("D", end='')
            elif y-i == (size//2-2) - (x-j):
                print("*", end='')
            elif i == y-1 or j == x-1:
                print("*", end='')
            elif i-y == j-x:
                print("*", end='')

            else:
                print(".", end='')
        print()
0.9461994076999013
def queen(x, y, size=8):
    for row in range(size):
        for col in range(size):
            if row + 1 == y and col + 1 == x:
                print("D", end="")
            elif row + 1 == y or col + 1 == x:
                print("*", end="")
            elif y - row + 1 == x - col + 1:
                print("*", end="")
            elif y - row + 1 == col - x + 3:
                print("*", end="")
            else:
                print(".", end="")
        print()
0.8795656465942744
def queen(x, y, size=8):
    x, y = x-1, y-1
    for row in range(size):
        for col in range(size):

            if row == y and col == x:
                print("D", end="")

            elif row == y or col == x:
                print("*", end="")

            elif y - row == x - col:
                print("*", end="")

            elif y - row == col - x:
                print("*", end="")

            else:
                print(".", end="")

        print()


0.7152023692003948
def queen(x, y):
    for rows in range(1,9):
        for cols in range(1,9):
            if cols==x and rows==y:
                print("D",end="")
            elif cols==x or rows==y:
                print("*",end="")
            elif cols-rows==x-y or cols+rows==x+y:
                print("*",end="")
            else:
                print(".", end="")
        print()

0.7107601184600197
def queen(x, y):
    for yd in range(1, 9):
        for xd in range(1, 9):
            if xd == x and yd == y:
                print("D", end="")
            elif xd == x or yd == y:
                print("*", end="")
            elif yd - xd + x == y or xd + yd - y == x:
                print("*", end="")
            else:
                print(".", end="")
        print()

