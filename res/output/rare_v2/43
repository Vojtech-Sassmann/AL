0.883111362992349
def odd_even(n):
    for i in range(1, n+1):
        if (i % 2 == 1):
            print(i, end=" ")
    print()
    for i in range(1, n+1):
        if (i % 2 == 0):
            print(i, end = "")
        else:
            print(" ", end = "")
    print()

