def print_sums(n):
    for i in range(1,n):
        n-=1
        print(i+n, "=", i, "+", n)

0.8832335329341318
def print_sums(n):
    for i in range(n):
        if i < (n-1):
            print(n, "=", (i+1), "+", (n-i-1))
            

0.8383233532934132
def print_sums(n):
    for i in range(1, n):
        if i + (n-i) == n:
            print(n, "=", i, "+", n - i)

