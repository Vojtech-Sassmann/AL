1.4670658682634732
def print_sums(n):
    for i in range(1,n):
        for j in range(1,n):
            if i+j == n:
                print("{} = {} + {}".format(n,i,j))
        

1.2275449101796407
def print_sums(n):
    x=n-1
    for i in range(n-1):
        print(n, "=", n-x, "+", x)
        x-=1
     

1.2125748502994012
def print_sums(n):
    for i in range(n-1):
        print(n, "=", (n+1)-(n-i), "+", n-((n+1)-(n-i)))

0.9880239520958084
def print_sums(n):
    k=n
    
    for i in range(n-1):
        print(n, "=", i+1, "+", k-1)
        k=k-1

