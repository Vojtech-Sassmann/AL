def pyramid(n):
    for i in range(1, n+1):
        print('  '*(n-i), "# "*i, end='')
        print('# '*(i-1))
    

0.8857203210815378
def pyramid(n):	
    for i in range(n):
        for j in range(2*n-1):
            if (abs(n-1-j)<=i):
                print("#",end=" ")
            else:
                print(" ",end=" ")
        print()
        
