0.8502323616392057
def pyramid(n):
    num_hashes = 1
    for num_spaces in range(n, 0, -1):
        for _ in range(1, num_spaces):
            print(" ", end=" ")
        for _ in range(num_hashes):
            print("#", end=" ")
        num_hashes += 2
        print()

