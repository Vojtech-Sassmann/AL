def pyramid(n):
    #if n == 1:
    print((2*n) * " " + "#")
    for i in range(1,n):
            print((2*n-2*i) * " " +(2 * i * "# ")+ "#")
    print()	

1.1533586818757922
def pyramid(n):
    x=n
    y=1
    for i in range(n):
        for i in range(x-1):
            print(" ",end=" ")
        for i in range(y):
            print("#",end=" ")
        for i in range(x-1):
            print(" ",end=" ")
        print()
        x-=1
        y+=2

1.1252640473172792
def pyramid(n):
    s=1
    q=n
    p=n*2
    for i in range(n):
        print(" "*(p//2) ," "*q , end="")
        print("# "*s)
        s+=2
        p-=2
        q-=1
1.1163920574566961
def pyramid(n):
	for i in range(n):
		for j in range(n*2-1):
			if (j+1 <= (n+(i)) and j+1 >= (n-(i))):
				print("#", end=" ")
			else:
				print(' ', end=' ')
		print()
1.1060414026193492
def pyramid(n):
    a=n-1
    b=n-1
    for j in range(n):
        for i in range(2*n-1):
            if a<=i<=b:
                print("#", end=" ")
            else:
                print(" ", end=" ")
        a=a-1
        b=b+1
        print()	

1.1016054076890578
def pyramid(n):
    for i in range(n):
        for j in range(2 * n - 1):
            if j >= n - 1 - i and j <= n - 1 + i:
                print('#', end=" ")
            else:
                print(' ', end=" ")
        print()
