def pyramid(n):
    h=1
    #Å™adky
    for i in range (1,n+1):
        #mezery
        for j in range (n-i):
            print(" ", end=" ",)
            
        #hashe
        
        for k in range(1,h+1):
            print ("#", end=" ")
        h+=2  
        print()
            
