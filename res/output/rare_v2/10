2.280101394169835
def firstLine(n):
    print(2*(n-1)*" ", end="")
    print("#", end="")
    print(2*(n-1)*" ", end="")
    print()

def pyramid(n):
    firstLine(n)
    for i in range(1,n):
        print((n-1-i)*"  ", end="")
        print((2*i+1)*"# ", end=" ")
        print((n-1-i)*"  ", end="")
        print()

1.3544571187156738
def pyramid(n):
	for i in range(n):
		for j in range(n):
			if j<n-i-1: print(" ", end=" ")
			else: print("#", end=" ")
		for j in range(n-1):
			if j<i: print("#", end=" ")
			else: print(" ", end=" ")
		print()	

1.2672158850866073
def pyramid(n):
    n1=1
    n2=1
    n3=1
    for x in range(n):
        for z in range(n-n1):
            print(" ",end=" ")
        for e in range(n2):
            print("#",end=" ")
        for a in range(n-n3):
            print(" ",end=" ")
            
        n1+=1
        n3+=1
        n2+=2
        print()
