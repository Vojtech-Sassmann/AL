def magic_test(tears, horns, tails):
    if tails>2:
        if tears>2:
            return True
        else:
            return False
    else:
        if horns>0:
            if tears>4:
                return True
            else:
                return False
        else:
            return False

1.196951219512195
def magic_test(tears, horns, tails):
    if tears < 5 or horns < 1:
        if tails < 3 or tears < 3:
            return False
        else:
            return True
    else:
        return True

