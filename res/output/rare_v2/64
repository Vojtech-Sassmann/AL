def magic_test(tears, horns, tails):
    # 5 slz fenixe a 1 roh
    # 3 slzy fenixe 0 rohu a 3 ocasky
    if tears >= 5 and horns >= 1:
        return True
    elif tears >= 3 and tails >= 3:
        return True
    else:
    	return False

