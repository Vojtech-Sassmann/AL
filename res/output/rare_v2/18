def digit_sum(n):
    x = 0
    z = int(len(str(n)))
    y = str(n)
    for i in range(z):
        x = x + int(y[i])
    return(print(x))




1.3628362670209233
def digit_sum(n):
	num = str(n)
	return sum([int(i) for i in num])

1.3435735636001327
def digit_sum(n):
    counter=0
    string=str(n)
    length=len(string)
    for i in range(length):
        number=string[i]
        real_number=int(number)
        counter=counter+real_number
        
    return counter
	
1.194287612089007
def digit_sum(n):
    if n == 0:
        return 0
    count = 0
    num = n
    for no in range(len(str(n))):
        count += num % 10
        num = num // 10
    return count
1.170209232813019

def digit_sum(n):
    x = 0
    count = list(map(int, str(n)))
    for i in range(len(count)):
        x += count[i]
    return x

