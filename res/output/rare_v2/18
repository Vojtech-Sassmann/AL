def digit_sum(n):
    n=str(n)
    b=0
    for i in range(len(n)):
        a=n[i]
        a=int(a)
        b+=a
    return b

1.1557622052474261
def digit_sum(n):
    sum = 0
    while n >= 10:
        sum += n % 10
        n //= 10
    sum += n
    return sum

