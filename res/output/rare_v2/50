def cross(z):
    for i in range(3):
        for j in range(3):
            if i==j or i+j==2 :
                print(z, end=' ')
            else:
                print(' ', end=' ')
        print()


1.0516201117318436
def cross(z):
    for i in range(3):
        if i == 0 or i == 2:
            print(z, " ", z, end=" ") 
            print()
        else:
            print(" ", z, " ", end=" ")
            print()


