1.6239106145251396
def cross(z):
    for i in range(3):
        for j in range(3):
            if i == 0 and j == 2 or i == 2 and j == 0 or j == 1 and i ==1 or i == 0 and j == 0 or i == 2 and j==2:
                print(z, end=' ')
            else:
                print(' ', end=' ')
        print()
1.612290502793296
def cross(z):
    for i in range(3):
        for j in range(3):
            if (j==0 and (i==0 or i==2)) or (i==1 and j==1) or (j==2 and (i==2 or i==0)):
                print(z, end=" ")
            else:
                print(" ", end=" ")
        print()
1.4118435754189944
def cross(z):
    for i in range (1,10):
        if i%2 == 0:
            print("  ", end = "")
        elif i%2==1:
            print(z, end = " ")
        if i%3 == 0:
            print("")
        i -= 1
