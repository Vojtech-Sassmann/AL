def power_test(a, b):
    if a>b:
        if(b**2==a): print(a, "je druhou mocninou", b)
        else: print("bagr")
    elif b>a:
        if(a**2==b): print(b, "je druhou mocninou", a)
        else: print("bagr")
    else: print("bagr")

1.1341765191091557
def power_test(a, b):
    mocA=a*a
    mocB=b*b
    
    if (a == mocB):
        print ("%d je druhou mocninou %d" %(a,b))
    elif (b == mocA):
        print ("%d je druhou mocninou %d" %(b,a))
    else:
        print("bagr")

