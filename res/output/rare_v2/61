    if a == b*b:
        print (b*b, "je druhou mocninou", a//b)
    elif b == a*a:
        print (a*a, "je druhou mocninou", b//a)
    else:
    	print("bagr")

0.9141462744019796
def power_test(a, b):
    A = a * a
    B = b * b
    if a == B:
        print(a, "je druhou mocninou", b)
    elif A == b:
        print(b, "je druhou mocninou", a)
    else:
    	print("bagr")

