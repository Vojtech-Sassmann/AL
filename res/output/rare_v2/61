0.9708551003574374
def power_test(a, b):
    vetsiCislo = a
    mensiCislo = b
    if(vetsiCislo < b):
        vetsiCislo = b
        mensiCislo = a
  
    if(vetsiCislo == mensiCislo**2):
        print(vetsiCislo, "je druhou mocninou", mensiCislo)
    else:
        print("bagr")


