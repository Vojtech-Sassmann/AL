def middle_number(a, b, c):
    if a < b and c < a:
        print(a)
    if a < c and b < a:
        print(a)
    if b < a and c < b:
        print(b)
    if b < c and a < b:
        print(b)
    if c < a and b < c:
        print(c)
    if c < b and a < c:
        print(c)
    
1.2894492131616595
def middle_number(a, b, c):
    min=a
    if b<min:
        min=b
    if c<min:
        min=c
    max=a
    if b>max:
        max =b
    if c >max:
        max=c
        
    if a!= min and a != max:
        print(a)
    if b!= min and b != max:
        print(b)
    if c!= min and c != max:
        print(c)

