def middle_number(a, b, c):
    d=int(max(a,b,c))
    e=int(min(a,b,c))
    g=(a+b+c)
    e=(g-(d+e))
    print(e)

0.9146995708154507
def middle_number(a, b, c):
    minus = min(a, b, c)
    plus = max(a, b, c)
    if a != minus and a != plus:
        print(a)
    elif b != minus and b != plus:
        print(b)
    elif c != minus and c != plus:
        print(c)
    

