def decide(symbol1, symbol2):\n    if symbol1 == symbol2:\n        print("Remiza")\n    elif symbol1 == "K" and symbol2 == "P" or symbol2 == "K" and symbol1 == "P":\n        print("P")\n    elif symbol1 == "N" and symbol2 == "P" or symbol2 == "N" and symbol1 == "P":\n        print("N")\n    elif symbol1 == "K" and symbol2 == "N" or symbol2 == "K" and symbol1 == "N":\n        print("K")\n
